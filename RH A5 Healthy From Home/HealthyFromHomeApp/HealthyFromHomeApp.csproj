<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net8.0-windows</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UseWPF>true</UseWPF>
		<OutputPath>bin\Debug\net8.0-windows</OutputPath>
		<BaseOutputPath>bin\Debug\net8.0-windows</BaseOutputPath>
	</PropertyGroup>

	<ItemGroup>
		<Page Remove="MainWindow.xaml" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Views\" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\BikeLibrary\BikeLibrary.csproj" />
	</ItemGroup>

	<ItemGroup>
		<Reference Include="BLELibrary">
			<HintPath>Libraries\BLELibrary.dll</HintPath>
		</Reference>
	</ItemGroup>

	<ItemGroup>
		<Compile Update="Clients\ClientMainWindow.xaml.cs">
			<SubType>Code</SubType>
		</Compile>
		<Compile Update="Doctor\DoctorMainWindow.xaml.cs">
			<SubType>Code</SubType>
		</Compile>
	</ItemGroup>

</Project>